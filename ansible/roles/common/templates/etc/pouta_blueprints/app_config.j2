DEBUG: {{ application_debug_logging }}
SECRET_KEY: '{{ application_secret_key }}'
SQLALCHEMY_DATABASE_URI: '{{ application_database_uri }}'
M2M_CREDENTIAL_STORE: '{{ application_m2m_credentials_path }}'
{%  if public_ipv4 is defined %}
PUBLIC_IPV4: '{{ public_ipv4 }}'
{%  if docker_https_port is defined and docker_https_port!=443 %}
BASE_URL: 'https://{{ public_ipv4 }}:{{ docker_https_port }}/'
{% else %}
BASE_URL: 'https://{{ public_ipv4 }}/'
{% endif %}
{% endif %}
PUBLIC_HTTPS_PROXY_PORT: {{ public_https_proxy_port }}
{% if apache_enable_ssl %}
INTERNAL_API_BASE_URL: 'https://www:{{ docker_https_port_worker }}/api/v1'
{% else %}
INTERNAL_API_BASE_URL: 'http://www:{{ docker_http_port_worker }}/api/v1'
{% endif %}

INTERNAL_HTTPS_PROXY_PORT: {{ internal_https_proxy_port }}
{% if enable_shibboleth | bool %}
ENABLE_SHIBBOLETH_LOGIN: True
{% endif %}
PROVISIONING_NUM_WORKERS: {{ provisioning_num_workers }}
