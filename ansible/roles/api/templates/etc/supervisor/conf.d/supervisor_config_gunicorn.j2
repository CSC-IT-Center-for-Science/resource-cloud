[program:gunicorn_app]
command = {{ virtualenv_path }}/bin/gunicorn_start
directory = {{ application_path }}
user = {{ application_user }}
stdout_logfile = {{ application_log_file }}
redirect_stderr = true

[program:gunicorn_worker_app]
command = {{ virtualenv_path }}/bin/gunicorn_worker_start
directory = {{ application_path }}
user = {{ application_user }}
stdout_logfile = {{ application_worker_endpoint_log_file }}
redirect_stderr = true
