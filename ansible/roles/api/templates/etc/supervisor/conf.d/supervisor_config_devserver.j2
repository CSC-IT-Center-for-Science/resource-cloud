[program:devserver_app]
command = {{ virtualenv_path }}/bin/devserver_start
directory = {{ application_path }}
user = {{ application_user }}
stdout_logfile = {{ application_log_file }}
redirect_stderr = true

[program:devserver_worker_app]
command = {{ virtualenv_path }}/bin/devserver_worker_start
directory = {{ application_path }}
user = {{ application_user }}
stdout_logfile = {{ application_worker_endpoint_log_file }}
redirect_stderr = true
